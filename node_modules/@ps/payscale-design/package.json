{
  "name": "@ps/payscale-design",
  "version": "3.5.4",
  "description": "PayScale Design.",
  "main": "lib/",
  "scripts": {
    "test": "mocha -R min -r babel-register --reporter progress ./testutils/dom.js components/*/tests/*.js",
    "lint": "eslint --config .eslintrc --format table components",
    "lint-fix": "eslint --fix --config .eslintrc --format table components",
    "teamcity-test": "mocha -R min -r babel-register --reporter mocha-teamcity-reporter ./testutils/dom.js components/*/tests/*.js",
    "teamcity-lint": "eslint --config .eslintrc --format ./node_modules/eslint-teamcity/index.js components",
    "compile": "npm run webpack && gulp",
    "webpack": "webpack && webpack --config webpack.config.styles.js",
    "prepublish": "npm run teamcity-lint && npm run teamcity-test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/payscale/payscale-design"
  },
  "keywords": [
    "ui",
    "ux",
    "frontend",
    "design",
    "reactjs",
    "react"
  ],
  "author": "nicolec",
  "license": "Unlicense",
